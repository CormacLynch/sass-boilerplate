		"autoprefixer": "^9.1.5",
		"babel-loader": "^8.0.2",
		"clean-webpack-plugin": "^0.1.19",
		"css-loader": "^1.0.0",
		"cssnano": "^4.1.0",
		"file-loader": "^2.0.0",
		"html-webpack-plugin": "^3.2.0",
		"http-server": "^0.11.1",
		"mini-css-extract-plugin": "^0.4.2",
		"node-sass": "^4.9.3",
		"postcss-import": "^12.0.0",
		"postcss-loader": "^3.0.0",
		"sass-loader": "^7.1.0",
		"style-loader": "^0.23.0",
