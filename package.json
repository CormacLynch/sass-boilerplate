{
	"name": "sass-boilerplate",
	"version": "1.0.0",
	"description": "Sass Boilerplate",
	"private": true,
	"scripts": {
		"start": "NODE_ENV=dev webpack-dev-server --open --port 3000",
		"watch": "NODE_ENV=dev webpack --colors --watch webpack-dev-server --open --port 3000",
		"prod": "NODE_ENV=prod webpack --colors",
		"demo": "http-server ./dist -o -p 3001",
		"clean": "rimraf node_modules && yarn cache clean"
	},
	"author": "Cormac Lynch",
	"license": "ISC",
